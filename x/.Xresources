! urxvt

! font
URxvt.font: xft:monospace
! scrollbar
URxvt.scrollBar: false
! requires urxvt-perls to be installed
URxvt.perl-ext-common: selection-to-clipboard

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! rofi

rofi.font:           sans-serif 20
rofi.fullscreen:     true
rofi.color-enabled:  true
rofi.padding:        400
rofi.opacity:        95
rofi.color-window:   #32302f, #32302f, #32302f
rofi.color-normal:   #32302f, #928374, #32302f, #32302f, #ebdbb2
rofi.color-active:   #32302f, #ebdbb2, #32302f, #32302f, #ebdbb2
rofi.color-urgent:   #32302f, #ebdbb2, #32302f, #32302f, #ebdbb2