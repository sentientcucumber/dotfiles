! Personal theme based on the Dracula theme.

#define black #282a36
#define white #f8f8f2

*.foreground: white
*.background: black

! Black
*.color0:     black
! Red
*.color1:     #ff5555
! Green
*.color2:     #50fa7b
! Yellow
*.color3:     #f1fa8c
! Blue
*.color4:     #bd93f9
! Magenta
*.color5:     #ff79c6
! Cyan
*.color6:     #8be9fd
! White
*.color7:     white
! Bright black
*.color8:     #4a4e64
! Bright red
*.color9:     #ff6e67
! Bright green
*.color10:    #5af78e
! Bright yellow
*.color11:    #f4f99d
! Bright blue
*.color12:    #caa9fa
! Bright magenta
*.color13:    #ff92d0
! Bright cyan
*.color14:    #9aedfe
! Bright white
*.color15:    #e6e6e6

URxvt.font:      xft:monospace:size=12
URxvt.scrollBar: false
URxvt.saveLines: 10000

rofi.color-normal:    argb:00282a36, #f8f8f2, argb:00373844, #44475a, #e2e2dc
rofi.color-urgent:    argb:00282a36, #ff79c6, argb:00373844, #ff79c6, #282a36
rofi.color-active:    argb:00282a36, #50fa7b, argb:00373844, #50fa7b, #282a36
rofi.color-window:    argb:ee282a36, #282a36, #f4f99d
rofi.separator-style: solid
rofi.font:            Montserrat 12
rofi.padding:         12